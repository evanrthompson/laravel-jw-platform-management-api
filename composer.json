{
    "name": "webstarters/laravel-jw-platform-management-api",
    "description": "A simple wrapper for the JW Platform Management API",
    "keywords": [
        "webstarters",
        "laravel",
        "jw-player",
        "jw-platform",
        "jw-platform-management"
    ],
    "homepage": "https://github.com/webstarters/laravel-jw-platform-management-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Peter JÃ¸rgensen",
            "email": "pj@webstarters.dk",
            "homepage": "https://webstarters.dk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1",
        "illuminate/support": "~5.7.0|~5.8.0|~6.0",
        "guzzlehttp/guzzle": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "Webstarters\\Platform\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webstarters\\Platform\\ManagementServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
